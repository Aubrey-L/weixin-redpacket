
page {
  --white: white;
  --reds: red;
  --BackColor:#f5f5f5;
  --blueTitle:#2967ff;
  --blueborder: #087ae8;
  --blue69: #6979f8;
  --grayborder:#E0E0E0;
  --gray999:#999999;
  --grayA2:#a2a2a2;
  --graysizeColor:#333;
  --grayCCC:#ccc;
  --green6b:#6BD39E;
  --size22:22rpx;
  --size24:24rpx;
  --size28:28rpx;
  --size30:30rpx;
  --size35:35rpx;
  -webkit-overflow-scrolling: touch;
  display: flex;
  flex-direction: column;
  width: 100%;
  height: 100%;
}

.text-text{
  color: #333333;
  font-size: 20rpx;
  margin:0rpx 78rpx 20rpx 78rpx;
}

button:not([size='mini']){
  width: 100% !important;
}

button {
  padding: 0rpx !important;
}
.submit {
  background-color: #2967ff;
  font-size: 26rpx;
  height: 98rpx;
  line-height: 98rpx;
  color: white;
  margin-bottom: 20rpx;
}
.submit-red {
  background-color: #DC6372;
  font-size: 26rpx;
  height: 98rpx;
  line-height: 98rpx;
  color: #FFDAAB;
  margin-bottom: 20rpx;
}